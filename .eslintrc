{
  "extends": [
    "airbnb-typescript/base",
    "plugin:@typescript-eslint/recommended"
  ],
  "env": {
    "node": true,
    "browser": false,
    "jest": true
  },
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaVersion": 2019
  },
  "rules": {
    "@typescript-eslint/comma-dangle": "off",
    "import/prefer-default-export": "off"
  }
}